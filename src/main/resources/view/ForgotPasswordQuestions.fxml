         <font>
            <Font size="13.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="back" layoutX="26.0" layoutY="428.0" mnemonicParsing="false" onAction="#backOnAction" prefHeight="35.0" prefWidth="65.0" style="-fx-background-radius: 10;" stylesheets="@../style/Loging.css" text="Back">
         <font>
            <Font size="13.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
