<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="489.0" prefWidth="383.0" style="-fx-background-color: #ffffff;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.test.controller.ForgotPasswordQuestionsController">
   <children>
      <Label layoutX="26.0" layoutY="22.0" text="Answer To All Qestion To Get Your Password" textFill="#007aff">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
      <Label layoutX="27.0" layoutY="48.0" text="Please answer the security question to get your password and" textFill="#00000081">
         <font>
            <Font size="11.0" />
         </font>
      </Label>
      <Label layoutX="27.0" layoutY="62.0" text="and account back and safe loging" textFill="#00000080">
         <font>
            <Font size="11.0" />
         </font>
      </Label>
      <Label layoutX="27.0" layoutY="120.0" text="01. Who is your childhood favourite teacher? ">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <TextField fx:id="fquestion" layoutX="45.0" layoutY="157.0" prefHeight="25.0" prefWidth="302.0" stylesheets="@../style/Loging.css" />
      <Label layoutX="27.0" layoutY="210.0" text="02. Who is your childhood best friend? ">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <TextField fx:id="squestion" layoutX="45.0" layoutY="246.0" prefHeight="25.0" prefWidth="302.0" stylesheets="@../style/Loging.css" />
