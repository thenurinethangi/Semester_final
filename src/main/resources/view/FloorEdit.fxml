<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="305.0" prefWidth="427.0" style="-fx-background-color: ECEFF1;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.test.controller.FloorEditController">
   <children>
      <Pane layoutX="13.0" layoutY="21.0" prefHeight="228.0" prefWidth="400.0" style="-fx-background-color: #ffffff; -fx-border-radius: 5; -fx-border-color: #747d8c; -fx-border-width: 0 1 1 1;">
         <children>
            <Label layoutX="3.0" layoutY="-10.0" text="Update Floor">
               <font>
                  <Font name="Candara Bold" size="18.0" />
               </font>
            </Label>
            <Label layoutX="14.0" layoutY="41.0" prefHeight="20.0" prefWidth="65.0" text="Floor No:" textFill="#747d8c">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <Label fx:id="floorNoLabel" layoutX="122.0" layoutY="42.0" prefHeight="20.0" prefWidth="20.0" text="1">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
            <Label layoutX="14.0" layoutY="91.0" prefHeight="20.0" prefWidth="86.0" text="No Of Houses:" textFill="#747d8c">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <ComboBox fx:id="houseAmountcmb" layoutX="120.0" layoutY="88.0" onAction="#houseAmountcmbOnAction" prefWidth="150.0" promptText="Select" styleClass="combo-box" stylesheets="@../style/AddNewUnit.css" />
            <Button fx:id="editbtn" layoutX="341.0" layoutY="241.0" mnemonicParsing="false" onAction="#editOnAction" prefHeight="29.0" prefWidth="57.0" style="-fx-background-color: #007AFF;" stylesheets="@../style/AddNewUnit.css" text="Edit" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
         </children>
      </Pane>
      <Button fx:id="clearbtn" layoutX="294.0" layoutY="263.0" mnemonicParsing="false" onAction="#clearOnAction" prefHeight="30.0" prefWidth="57.0" stylesheets="@../style/AddNewUnit.css" text="Clear">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="canclebtn" layoutX="234.0" layoutY="263.0" mnemonicParsing="false" onAction="#cancleOnAction" prefHeight="30.0" prefWidth="57.0" stylesheets="@../style/AddNewUnit.css" text="Cancle">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
